{
  "name": "playlist-maker",
  "version": "0.0.1",
  "license": "GPL-3",
  "author": "Denis Zholob",
  "description": "Playlist Maker App",
  "private": true,
  "engines": {
    "node": ">= 14.17.0",
    "npm": ">= 6.14.13"
  },
  "scripts": {
    "i": "npm i --no-package-lock",
    "gen-combined-coverage": "node ./tools/scripts/coverage/merge-coverage.js",
    "start": "nx serve --port 9200",
    "start:web-prod": "http-server -p 8080 -c-1 dist/apps/web",
    "start:electron": "nx serve electron",
    "build:web": "nx run web:build:production --baseHref='./'",
    "build:electron": "nx run electron:build:production",
    "electron:package": "nx run electron:package",
    "electron:make": "nx run electron:make",
    "build-and-package": "nx run web:build:production --baseHref='./' && nx run electron:build:production && nx run electron:package",
    "build-and-make": "nx run web:build:production --baseHref='./' && nx run electron:build:production && nx run electron:make",
    "test": "nx run-many --all --target=test --parallel --codeCoverage",
    "test:force-regenerate": "nx run-many --all --target=test --parallel --codeCoverage --skip-nx-cache",
    "test:view-coverage": "npm run test && npm run gen-combined-coverage && npm run view-coverage",
    "test:web": "nx run web:test --codeCoverage && npm run view-coverage:web",
    "test:electron": "nx run electron:test --codeCoverage && npm run view-coverage:electron",
    "test:components": "nx run components:test --codeCoverage && npm run view-coverage:components",
    "test:util": "nx run util:test --codeCoverage && npm run view-coverage:util",
    "view-coverage": "cd coverage/report && index.html",
    "view-coverage:electron": "cd coverage/apps/electron && index.html",
    "view-coverage:web": "cd coverage/apps/web && index.html",
    "view-coverage:components": "cd coverage/apps/components && index.html",
    "view-coverage:util": "cd coverage/apps/util && index.html",
    "lint": "nx workspace-lint && nx lint",
    "lint:all": "nx run-many --target=lint --all",
    "lint:all-err": "nx run-many --target=lint --all --quiet",
    "lint:fix": "nx lint --fix",
    "lint:err": "nx lint --quiet",
    "e2e": "nx e2e",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "format:all": "nx format:write --all",
    "dep-graph": "nx dep-graph",
    "update": "nx migrate latest",
    "ng": "nx",
    "nx": "nx",
    "pre-commit": "npm version patch --no-git-tag-version && git add package.json && npx lint-staged",
    "prepare": "husky install",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && exitzero electron-builder install-app-deps"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --cache --fix"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@angular-slider/ngx-slider": "^2.0.3",
    "@angular/animations": "^12.2.0",
    "@angular/cdk": "^12.2.12",
    "@angular/common": "^12.2.0",
    "@angular/compiler": "^12.2.0",
    "@angular/core": "^12.2.0",
    "@angular/forms": "^12.2.0",
    "@angular/platform-browser": "^12.2.0",
    "@angular/platform-browser-dynamic": "^12.2.0",
    "@angular/router": "^12.2.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@nrwl/angular": "12.10.0",
    "@types/mime-types": "^2.1.1",
    "balloon-css": "^1.2.0",
    "buffer": "^6.0.3",
    "moment": "^2.29.1",
    "music-metadata-browser": "^2.5.0",
    "papercss": "^1.8.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "^12.2.0",
    "@angular/language-service": "^12.2.0",
    "@nrwl/cli": "12.10.0",
    "@nrwl/cypress": "12.10.0",
    "@nrwl/eslint-plugin-nx": "12.10.0",
    "@nrwl/jest": "12.10.0",
    "@nrwl/linter": "12.10.0",
    "@nrwl/tao": "12.10.0",
    "@nrwl/workspace": "12.10.0",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "cypress": "^8.3.0",
    "electron": "^13.2.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "exitzero": "^1.0.1",
    "husky": "^7.0.4",
    "jest": "27.2.3",
    "jest-preset-angular": "10.0.1",
    "lint-staged": "^11.2.6",
    "nx-electron": "^11.4.0",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.3.5"
  }
}
