{
  "npmScope": "plm",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "components": {
      "tags": ["scope:web", "type:ui"]
    },
    "electron": {
      "tags": ["scope:electron"],
      "implicitDependencies": ["web"]
    },
    "pages": {
      "tags": ["scope:web", "type:feature"]
    },
    "util": {
      "tags": ["scope:web", "scope:electron", "type:util"]
    },
    "web": {
      "tags": ["scope:web"]
    },
    "web-e2e": {
      "tags": ["scope:web"],
      "implicitDependencies": ["web"]
    }
  }
}
